# $Id$

  prompt "unsubscribe probe:    "

  DEFAULT="unsubscribe"
  LOCAL="${LOC}-${DEFAULT}"
  file_run ezmlm-manage "$DIR" || \
  fatal "failed to produce unsubscribe probe"

  grephdr_manage
  grephdr reply-to: "${LOC}-uc\\..*\\..*-test=example.org@${HOST}"
  grephdr subject: "confirm unsubscribe from ${LOC}@${HOST}"
  grephdr_empty

  ${ECHO} OK

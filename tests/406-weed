# $Id$

prompt "dispatch weed:        "

SENDER=user@host
MAKE_HEADER="Precedence: bulk"
run ezmlm-make -+y "$DIR"

dispatch && fatal "ezmlm-weed was not run in the editor"

dispatch confirm && fatal "ezmlm-weed was not run in the confirmer"

dispatch return- && fatal "ezmlm-weed was not run in the bouncer"

dispatch accept- && fatal "ezmlm-weed was not run in the moderator"

dispatch blah && fatal "ezmlm-weed was not run in the manager"

MAKE_HEADER=""

${ECHO} "OK"

# $Id$

  prompt "subscribe:            "

  DEFAULT="subscribe"
  LOCAL="${LOC}-${DEFAULT}"
  file_run ezmlm-manage -S "$DIR" || \
  fatal "failed to produce subscribe confirmation"

  grephdr_manage
  grephdr list-unsubscribe: "<mailto:${LOC}-unsubscribe-test=example.org@${HOST}>"
  grephdr subject: "welcome to ${LOC}@${HOST}"
  grephdr_empty

  ${ECHO} OK

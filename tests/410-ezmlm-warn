# $Id$

##############
# ezmlm-warn #
##############

if [ "$SECT" -le "4" ]; then

  prompt "ezmlm-warn (3/3):     "

  SENDER="${BNC}@${HOST}"
  export SENDER
  ${EZBIN}/ezmlm-issubn -n "${DIR}" || \
	{ ${ECHO} "failed to remove bouncing subscriber"; exit 100; }
  ${EZBIN}/ezmlm-issubn -n "${DIR}/digest" || \
	{ ${ECHO} "failed to remove bouncing digest subscriber"; exit 100; }

  ${ECHO} "OK"

fi

# $Id$

##############
# ezmlm-warn #
##############

if [ "$SECT" -le "4" ]; then

  prompt "ezmlm-warn (3/3):     "

  SENDER="${BNC}@${HOST}"
  export SENDER
  ${EZBIN}/ezmlm-issubn -n "$DIR" || \
	fatal "failed to remove bouncing subscriber"
  ${EZBIN}/ezmlm-issubn -n "${DIR}/digest" || \
	fatal "failed to remove bouncing digest subscriber"

  ${ECHO} "OK"

fi

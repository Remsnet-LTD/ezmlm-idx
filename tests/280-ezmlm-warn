# $Id$

##############
# ezmlm-warn #
##############

if [ "$SECT" -le "2" ]; then

prompt "ezmlm-warn (2/3):     "

${EZBIN}/ezmlm-warn -t0 "${DIR}" >"${ERR}" 2>&1 || \
	fatal "failed with normal bounce for warning"

${EZBIN}/ezmlm-warn -d -t0 "${DIR}" >"${ERR}" 2>&1 || \
	fatal "failed with digest bounce for warning"

${ECHO} "OK"

fi
